	<queryString language="SQL">
		<![CDATA[select "Customer No" as col_key, coalesce(ac.customer_code, '') as col_value from accounts ac join part_tran pt on ac.acid=pt.acid join dtd dtd where pt.transaction_header_id=dtd.sn and pt.part_tran_type='Credit' and dtd.transaction_code=$P{transaction_code} union select "Account No" as col_key, coalesce(pt.acid, '') as col_value from part_tran pt join accounts ac on pt.acid=ac.acid join dtd dtd where pt.transaction_header_id=dtd.sn and pt.part_tran_type='Credit' and dtd.transaction_code=$P{transaction_code} union select "Account Name" as col_key, coalesce(ac.account_name, '') as col_value from part_tran pt join accounts ac on pt.acid=ac.acid  join dtd dtd where pt.transaction_header_id=dtd.sn and pt.part_tran_type='Credit' and dtd.transaction_code=$P{transaction_code} union select "Account Type" as col_key, coalesce(ac.account_type, '') as col_value from part_tran pt join accounts ac on pt.acid=ac.acid  join dtd dtd where pt.transaction_header_id=dtd.sn and pt.part_tran_type='Credit'  and dtd.transaction_code=$P{transaction_code} union select "Transaction Date" as col_key, pt.tran_date as tran_date from part_tran pt join accounts ac on pt.acid=ac.acid join dtd dtd where pt.transaction_header_id=dtd.sn and pt.part_tran_type='Credit' and dtd.transaction_code=$P{transaction_code} union
select "Transaction code" as col_key, $P{transaction_code} as col_value union
select "Amount" as col_key, coalesce(pt.tran_amount, 0.0) as col_value from part_tran pt join accounts ac on pt.acid=ac.acid join dtd dtd where pt.transaction_header_id=dtd.sn and pt.part_tran_type='Credit' and dtd.transaction_code=$P{transaction_code} union
select "Amount in Words" as col_key,coalesce($P{amount_in_words}, '') as col_value union
select "Transaction Charges" as col_key, cpt.tran_amount as col_value from charge_partran cpt join dtd dtd where cpt.transaction_header_id=dtd.sn and dtd.transaction_code=$P{transaction_code} union
select "Remarks" as col_key, coalesce(pt.tran_particulars, '') as col_value from part_tran pt join dtd dtd where pt.transaction_header_id=dtd.sn and pt.part_tran_type='Credit' and dtd.transaction_code=$P{transaction_code} union
select "Transaction Charges" as col_key, coalesce(cpt.tran_amount, '0') from charge_partran cpt join dtd dtd on dtd.sn=cpt.transaction_header_id where dtd.transaction_code=$P{transaction_code} union
select "Branch" as col_key, coalesce(concat(br.branch_code,'-',br.branch_description), '') as col_value from branch br join accounts ac on br.branch_code=ac.sol_code join part_tran pt on ac.acid=pt.acid join dtd dtd on pt.transaction_header_id=dtd.sn and dtd.transaction_code=$P{transaction_code} group by dtd.transaction_code]]>
