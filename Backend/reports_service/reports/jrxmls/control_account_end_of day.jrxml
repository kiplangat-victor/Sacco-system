	<queryString language="SQL">
		<![CDATA[SELECT 
    'Total Loans Disbursed' AS name, 
    COALESCE(SUM(total_loan_balance), 0) AS amount, 
    (SELECT COALESCE(disbursed_by, '') FROM loan WHERE DATE(disbursement_date) = DATE( $P{todaydate}) AND disbursed_by IS NOT NULL LIMIT 1) AS users
FROM loan
WHERE DATE(disbursement_date) = DATE( $P{todaydate})

UNION ALL

SELECT 'Total Principal Amount' AS name, COALESCE(SUM(ld.principal_amount), 0) AS amount, d.posted_by as users
FROM loan_demand ld
join dtd d on d.transaction_code = ld.transaction_code
WHERE DATE(ld.demand_date) = DATE( $P{todaydate}) AND ld.satisfaction_caller_flag = 'Y'

UNION ALL

SELECT 'Total Interest Amount' AS name, COALESCE(SUM(ld.interest_amount), 0) AS amount, d.posted_by as users
FROM loan_demand ld
join dtd d on d.transaction_code = ld.transaction_code
WHERE DATE(ld.demand_date) = DATE( $P{todaydate}) AND ld.satisfaction_caller_flag = 'Y'

UNION ALL

SELECT 'Total Cash Deposit' AS name, COALESCE(SUM(tran_amount), 0) AS amount, d.posted_by as users
FROM part_tran pt
join dtd d on d.sn = pt.transaction_header_id 
WHERE pt.tran_particulars LIKE '%cash deposit%' 
AND DATE(pt.tran_date) = DATE( $P{todaydate}) AND pt.part_tran_type = 'credit'

UNION ALL

SELECT 'Total Cash Withdrawal' AS name, COALESCE(SUM(pt.tran_amount), 0) AS amount, d.posted_by as users
FROM part_tran pt
join dtd d on d.sn = pt.transaction_header_id 
WHERE pt.tran_particulars LIKE '%cash withdrawal%' 
AND DATE(pt.tran_date) = DATE( $P{todaydate}) AND pt.part_tran_type = 'credit'

UNION ALL

SELECT 'Total Cheque Deposit' AS name, COALESCE(SUM(tran_amount), 0) AS amount, d.posted_by as users
FROM part_tran pt
join dtd d on d.sn = pt.transaction_header_id 
WHERE pt.tran_particulars LIKE '%cheque deposit%' 
AND DATE(pt.tran_date) = DATE( $P{todaydate}) AND pt.part_tran_type = 'credit'

UNION ALL

SELECT 'Total Paybill Deposit' AS name, COALESCE(SUM(tran_amount), 0) AS amount, d.posted_by as users
FROM part_tran pt 
join dtd d on d.sn = pt.transaction_header_id 
WHERE pt.tran_particulars LIKE '%paybill deposit%' 
AND DATE(pt.tran_date) = DATE( $P{todaydate}) AND pt.part_tran_type = 'credit'

UNION ALL

SELECT 'Total Paybill Withdrawal' AS name, COALESCE(SUM(tran_amount), 0) AS amount, d.posted_by as users
FROM part_tran pt
join dtd d on d.sn = pt.transaction_header_id 
WHERE pt.tran_particulars LIKE '%paybill withdrawal%' 
AND DATE(pt.tran_date) = DATE( $P{todaydate}) AND pt.part_tran_type = 'credit'

UNION ALL

SELECT acid, account_name, account_manager as users
FROM accounts
WHERE opening_date = DATE( $P{todaydate})]]>
	</queryString>
