	<queryString language="SQL">
		<![CDATA[select coalesce(ac.acid, '') as acid, coalesce(br.branch_description) as branch_description, coalesce(ac.account_name, '') as account_name, coalesce(rc.phone_number, '') as phone_number,
ln.disbursement_date, coalesce((ln.disbursement_amount), 0.0) as disbursement_amount,
coalesce(ac.account_balance, 0.0) as account_balance, coalesce(ac.account_manager, '') as account_manager, coalesce((sum(ld.demand_amount)-sum(ld.adjustment_amount)), 0.0) as arrears from accounts ac
join loan ln on ac.id=ln.account_id_fk join loan_demand ld on ln.sn=ld.loan_sn join branch br on ac.sol_code=br.branch_code
