	<queryString language="SQL">
		<![CDATA[select gl.gl_code as glCode, gl.gl_description as glName, glsubhead.gl_subhead_code as GlSubheadCode,
glsubhead.gl_subhead_description as GlSubheadName,accounts.acid as Account,
COALESCE(SUM(if(part_tran_type = 'Debit',tran_amount,0)),0) as totalDebit,
COALESCE(SUM(if(part_tran_type = 'Credit',tran_amount,0)),0) as totalCredit from gl
join glsubhead on gl.gl_code = glsubhead.gl_code
join accounts on accounts.gl_subhead = glsubhead.gl_subhead_code
left join part_tran on part_tran.acid = accounts.acid
where gl.gl_code=$P{glcode} 
group by glsubhead.gl_subhead_code]]>
	</queryString>
