	<queryString language="SQL">
		<![CDATA[SELECT * FROM (SELECT * FROM ( select coalesce(member.phone_number, '') as phone_number, CONCAT(coalesce(member.first_name, ''), ' ',coalesce(member.middle_name, ''), ' ' , coalesce(member.last_name, '')) as member_name, coalesce(member.customer_code, '') as customer_code, member.posted_time, coalesce(br.branch_description, '') as branch_description, coalesce(member.posted_by, '') as posted_by from retailcustomer member  join
branch br on br.branch_code=member.branch_code and member.deleted_flag='N' and br.deleted_flag='N' ) AS k

UNION

SELECT * FROM(select coalesce(member.primary_phone, '') as phone_number, coalesce(member.group_name, '') as member_name, coalesce(member.customer_code, '') as customer_code, member.posted_time, coalesce(br.branch_description, '') as branch_description, coalesce(member.posted_by, '') as posted_by from group_member member  join
branch br on br.branch_code=member.branch_code and member.deleted_flag='N' and br.deleted_flag='N' ) AS m
) AS t WHERE customer_code IN (SELECT customer_code FROM allgroupmembers WHERE group_member_fk IN (SELECT id FROM group_member where customer_code = $P{group_code}))
ORDER BY customer_code;]]>
