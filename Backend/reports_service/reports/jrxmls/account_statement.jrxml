	<queryString language="SQL">
		<![CDATA[SELECT *, rel_amount AS relative_amount  FROM(
SELECT gl.classification AS cls, prd.product_code, prd.product_code_desc, ac.account_name, ac.acid, ptdtd.dtd_sn, ptdtd.sn, ptdtd.transaction_code, ptdtd.tran_date, ptdtd.tran_particulars, ptdtd.c_amount, ptdtd.d_amount, ptdtd.rel_amount, ptdtd.tran_amount FROM (

SELECT acid, dtd_sn, sn, transaction_code,
 tran_date, tran_particulars, IF(rel_amount_inner > 0, rel_amount_inner, 0) AS c_amount,   IF(rel_amount_inner < 0, rel_amount_inner, 0) AS d_amount, rel_amount_inner AS rel_amount, ABS(rel_amount_inner) AS tran_amount FROM (select distinct pt.acid,  0 AS dtd_sn, 0 AS sn, 'Opening bal' AS transaction_code, 
$P{from_date} AS tran_date, 'Balance brought forward' AS tran_particulars,
SUM(CAST((case when pt.part_tran_type='Debit' then tran_amount*-1 else tran_amount end) AS DECIMAL(65, 2))) as rel_amount_inner  
from part_tran pt join dtd on pt.transaction_header_id = dtd.sn and dtd.posted_flag = 'Y' and pt.tran_date < $P{from_date} and acid =  $P{acid}  
  GROUP BY acid) AS k 

UNION

(select distinct pt.acid, pt.sn, dtd.sn as dtd_sn, dtd.transaction_code, 
pt.tran_date, tran_particulars,
CAST((case when pt.part_tran_type='Credit' then tran_amount else 0.00 end) AS DECIMAL(65, 2)) as c_amount,
CAST((case when pt.part_tran_type='Debit' then tran_amount else 0.00 end) AS DECIMAL(65, 2)) as d_amount,
CAST((case when pt.part_tran_type='Debit' then tran_amount*-1 else tran_amount end) AS DECIMAL(65, 2)) as rel_amount,
pt.tran_amount  
from part_tran pt join dtd on pt.transaction_header_id = dtd.sn and dtd.posted_flag = 'Y' and  pt.tran_date BETWEEN $P{from_date} AND DATE_ADD( $P{to_date}, INTERVAL 1 DAY) and acid =  $P{acid}  
 and (dtd.reversal_posted_flag  IS NULL OR dtd.reversal_posted_flag = 'N' OR (dtd.reversal_posted_flag <> 'Y' and dtd.reversed_flag <> 'Y')) )

) AS ptdtd

JOIN accounts AS ac ON ac.acid = ptdtd.acid and ac.acid = $P{acid}  
left join (SELECT product_code, product_code_desc FROM product) AS prd On ac.product_code = prd.product_code
join glsubhead ON glsubhead.gl_subhead_code = ac.gl_subhead
join gl ON gl.gl_code = glsubhead.gl_code
) AS k
order by  tran_date asc, dtd_sn ASC, tran_amount DESC]]>
	</queryString>
