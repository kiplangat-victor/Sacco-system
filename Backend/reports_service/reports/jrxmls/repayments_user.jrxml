	<queryString>
		<![CDATA[select coalesce(ac.acid, '') as acid, ln.disbursed_by ,coalesce(ld.adjustment_date) as adjustment_date, coalesce(ac.account_name, '') as account_name,
coalesce(ac.account_manager, '') as account_manager, coalesce(ld.adjustment_amount, 0.0) as adjustment_amount,
coalesce(br.branch_description, '') as branch_description, coalesce(ld.demand_type, '') as demand_type
from accounts ac join branch br on ac.sol_code=br.branch_code
join loan_demand ld on ac.acid=ld.acid
join loan ln on ln.sn=ld.loan_sn and ld.adjustment_date between $P{from_date} and $P{to_date} and ln.disbursed_by=$P{disbursed_by}]]>
