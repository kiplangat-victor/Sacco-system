	<queryString language="SQL">
		<![CDATA[select ac.acid, ac.account_name, ld.demand_date, ld.demand_amount, ld.sn, ld.demand_type, ld.adjustment_amount, rc.phone_number,
ac.account_manager, coalesce((sum(ld.demand_amount)-sum(ld.adjustment_amount)), 0.0) as arrears from accounts ac
 join loan_demand ld on ac.acid=ld.acid and ld.demand_amount > ld.adjustment_amount+1   
join retailcustomer rc on ac.customer_code=rc.customer_code where ac.delete_flag='N'
and rc.deleted_flag='N' and ld.demand_date <=(CURDATE() - INTERVAL 1 DAY) and ac.account_type='LAA'  
and ld.demand_date between $P{from_date} and $P{to_date} group by ld.sn order by ld.demand_date]]>
