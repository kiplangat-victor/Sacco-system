	<queryString language="SQL">
		<![CDATA[select coalesce(ac.acid, '') as acid, ln.asset_classification, coalesce(ac.account_name, '') as account_name, coalesce(rc.phone_number, '') as phone_number,
ln.disbursement_date, coalesce((ln.principal_amount), 0.0) as principal_amount,
coalesce(ac.account_balance, 0.0) as account_balance, coalesce(ac.account_manager, '') as account_manager, coalesce((sum(ld.demand_amount)-sum(ld.adjustment_amount)), 0.0) as arrears from accounts ac
join loan ln on ac.id=ln.account_id_fk left join loan_demand ld on ln.sn=ld.loan_sn
join  (SELECT * FROM (select unique_id  AS national_id, dob, coalesce(member.phone_number, '') as phone_number, CONCAT(coalesce(member.first_name, ''), ' ',coalesce(member.middle_name, ''), ' ' , coalesce(member.last_name, '')) as member_name, coalesce(member.customer_code, '') as customer_code, member.posted_time, coalesce(br.branch_description, '') as branch_description, coalesce(member.posted_by, '') as posted_by, coalesce(member.employer_code, '') as employer_code,  coalesce(member.employer_name, '') as employer_name  from retailcustomer member  join 
 branch br on br.branch_code=member.branch_code and member.deleted_flag='N' and br.deleted_flag='N' ) AS k
UNION
SELECT * FROM(select verification_doc_number AS national_id,  registration_date as dob,  coalesce(member.primary_phone, '') as phone_number, coalesce(member.group_name, '') as member_name, coalesce(member.customer_code, '') as customer_code, member.posted_time, coalesce(br.branch_description, '') as branch_description, coalesce(member.posted_by, '') as posted_by,  ' ' AS employer_code, ' ' AS employer_name from group_member member  join
branch br on br.branch_code=member.branch_code and member.deleted_flag='N' and br.deleted_flag='N' ) AS m) AS rc on ac.customer_code=rc.customer_code
and ln.disbursement_date between $P{from_date} and $P{to_date} group by ac.acid order by disbursement_date]]>
	</queryString>
