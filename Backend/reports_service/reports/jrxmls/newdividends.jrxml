	<queryString language="SQL">
		<![CDATA[SELECT
    a.customer_code,
    a.account_name,
    s01_balance.balance AS s01_balance,
    s01_balance.balance * 0.1 AS s01_dividends,
    CASE WHEN s02_balance.customer_code IS NOT NULL THEN s02_balance.balance ELSE NULL END AS s02_balance,
    CASE WHEN s02_balance.customer_code IS NOT NULL THEN s02_balance.balance * 0.04 ELSE NULL END AS s02_dividends  
FROM 
    accounts a
JOIN
    loan l ON a.id = l.account_id_fk
JOIN
    (SELECT 
         a.customer_code,
         SUM(CASE WHEN pt.part_tran_type = 'Debit' THEN -1 * pt.tran_amount ELSE pt.tran_amount END) AS balance
     FROM 
         part_tran pt
     JOIN 
         dtd ON pt.transaction_header_id = dtd.sn
     JOIN 
         accounts a ON a.acid = pt.acid
     WHERE 
         dtd.deleted_flag = 'N'
         AND dtd.posted_flag = 'Y'
         AND pt.tran_date <=  $P{to_date}
         AND a.product_code = 'S01'
     GROUP BY 
         a.customer_code
     HAVING 
         balance >= 5000
    ) AS s01_balance ON a.customer_code = s01_balance.customer_code
LEFT JOIN
    (SELECT 
         a.customer_code,
         SUM(CASE WHEN pt.part_tran_type = 'Debit' THEN -1 * pt.tran_amount ELSE pt.tran_amount END) AS balance
     FROM 
         part_tran pt
     JOIN 
         dtd ON pt.transaction_header_id = dtd.sn
     JOIN 
         accounts a ON a.acid = pt.acid
     WHERE 
         dtd.deleted_flag = 'N'
         AND dtd.posted_flag = 'Y'
         AND pt.tran_date <=  $P{to_date}
         AND a.product_code = 'S02'
     GROUP BY 
         a.customer_code
     HAVING 
         balance >= 5000
    ) AS s02_balance ON a.customer_code = s02_balance.customer_code
WHERE 
    a.customer_code IN (
        SELECT 
            customer_code
        FROM 
            accounts 
        WHERE 
            product_code = 'S01' 
            AND account_balance >= 5000
            AND customer_code IS NOT NULL 
    )
    AND a.account_type = 'LAA'
    AND (l.asset_classification IN ('PERFORMING', 'WATCH') OR a.account_balance >= -1)
GROUP BY
    a.customer_code;]]>
	</queryString>
