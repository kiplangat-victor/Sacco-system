	<queryString>
		<![CDATA[select ac.acid, ac.account_name, ld.demand_amount, ld.demand_type, br.branch_description, ld.adjustment_amount, rc.phone_number,
ac.account_manager, coalesce((sum(ld.demand_amount)-sum(ld.adjustment_amount)), 0.0) as arrears from accounts ac
join retailcustomer rc on ac.customer_code=rc.customer_code join
branch br on ac.sol_code=br.branch_code join loan_demand ld on ac.acid=ld.acid where ac.delete_flag='N'
and rc.deleted_flag='N' and ld.demand_date <=(CURDATE() - INTERVAL 1 DAY) and ac.account_type='LAA'
and ld.demand_date between $P{from_date} and $P{to_date} and br.branch_code=$P{sol_code}
