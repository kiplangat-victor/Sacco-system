COALESCE(SUM(if(pt.part_tran_type = 'Credit',pt.tran_amount,0)),0) as totalCredit from gl
join glsubhead gls on gl.gl_code=gls.gl_code
join accounts ac on ac.gl_subhead=gls.gl_subhead_code
