	<parameter name="to_date" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT 
    m.customer_code, 
    ac.acid, 
    ac.opening_date, 
    m.member_name,  
    COALESCE(m.employer_code, '') AS employer_code, 
    COALESCE(m.employer_name, '') AS employer_name, 
    m.phone_number, 
    ac.product_code, 
    0 AS interestrate, 
    ac.account_balance, 
    COALESCE(ga.g_amount,0) g_amount, 
    COALESCE(ga.loan_g_count,0) loan_g_count,
    (SELECT COALESCE((SELECT account_balance FROM accounts WHERE product_code = 'S02' AND customer_code = m.customer_code), 0)) AS S02_balance
FROM 
    (SELECT * FROM accounts where product_code = $P{product_code} and  opening_date between $P{from_date} and DATE_ADD($P{to_date}, INTERVAL 1 DAY)) ac
LEFT JOIN 
    (SELECT sum(guarantee_amount) as g_amount, count(guarantee_amount) AS loan_g_count, guarantor_customer_code 
     FROM loan_guarantor 
     WHERE loan_sn IN (SELECT sn FROM loan WHERE account_id_fk IN (SELECT id FROM accounts WHERE account_type = 'laa' AND account_balance < -1)) 
     GROUP BY loan_sn) AS ga 
ON ga.guarantor_customer_code = ac.customer_code
JOIN  
    (SELECT * FROM (SELECT coalesce(member.phone_number, '') as phone_number, CONCAT(coalesce(member.first_name, ''), ' ',coalesce(member.middle_name, ''), ' ' , coalesce(member.last_name, '')) as member_name, coalesce(member.customer_code, '') as customer_code, member.posted_time, coalesce(br.branch_description, '') as branch_description, coalesce(member.posted_by, '') as posted_by, employer_code, employer_name FROM retailcustomer member JOIN branch br ON br.branch_code=member.branch_code AND member.deleted_flag='N' AND br.deleted_flag='N') AS k
    UNION
    SELECT * FROM(SELECT coalesce(member.primary_phone, '') as phone_number, coalesce(member.group_name, '') as member_name, coalesce(member.customer_code, '') as customer_code, member.posted_time, coalesce(br.branch_description, '') as branch_description, coalesce(member.posted_by, '') as posted_by, '' AS employer_code, '' AS employer_name FROM group_member member JOIN branch br ON br.branch_code=member.branch_code AND member.deleted_flag='N' AND br.deleted_flag='N') AS m) AS m 
ON m.customer_code = ac.customer_code;
]]>
	</queryString>
