	<queryString>
		<![CDATA[select * from (
    (SELECT 
        classification, a.acid, a.account_name, a.gl_subhead, gl_subhead_description, round(SUM(Balance), 2) AS bal,
        CASE
            WHEN classification = 'ASSETS' THEN 'debit'
            WHEN classification = 'INCOMES' THEN 'debit'
            WHEN classification = 'LIABILITIES' THEN 'credit'
            WHEN classification = 'EXPENSES' THEN 'credit'
            WHEN classification = 'SHARES' THEN 'credit'
            WHEN classification = 'RESERVE' THEN 'debit'
            ELSE NULL
        END AS correct_column 
    FROM accounts a
    JOIN glsubhead b ON a.gl_subhead = b.gl_subhead_code
    JOIN gl c ON c.gl_code = b.gl_code
    JOIN (
    	SELECT 
    pt.acid,
    SUM(CASE 
            WHEN pt.part_tran_type = 'Debit' THEN -pt.tran_amount 
            ELSE pt.tran_amount 
        END) AS Balance
FROM 
    part_tran pt
JOIN 
    dtd ON pt.transaction_header_id = dtd.sn
JOIN 
    accounts a ON a.acid = pt.acid
WHERE 
    dtd.deleted_flag = 'N' 
    AND dtd.posted_flag = 'Y'
    AND pt.tran_date <= DATE_ADD($P{to_date}, INTERVAL 1 DAY)
    AND (
            dtd.reversal_posted_flag IS NULL
            OR dtd.reversal_posted_flag = 'N'
            OR (dtd.reversal_posted_flag <> 'Y' AND dtd.reversed_flag <> 'Y')
        )
    AND a.gl_subhead NOT IN (500, 220, 110, 250)
GROUP BY 
    pt.acid
    ) d ON a.acid = d.acid
   
    GROUP BY a.acid, a.gl_subhead) -- Added a.gl_subhead to grouping
UNION ALL
    (SELECT 
        classification, gl_subhead as acid, gl_subhead_description as account_name, gl_subhead, gl_subhead_description, round(SUM(Balance), 2) AS bal,
        CASE
            WHEN classification = 'ASSETS' THEN 'debit'
            WHEN classification = 'INCOMES' THEN 'debit'
            WHEN classification = 'LIABILITIES' THEN 'credit'
            WHEN classification = 'EXPENSES' THEN 'credit'
            WHEN classification = 'SHARES' THEN 'credit'
            WHEN classification = 'RESERVE' THEN 'debit'
            ELSE NULL
        END AS correct_column 
    FROM accounts a
    JOIN glsubhead b ON a.gl_subhead = b.gl_subhead_code
    JOIN gl c ON c.gl_code = b.gl_code
    JOIN (SELECT a.acid, SUM(CASE WHEN part_tran_type = 'Debit' THEN -1 * tran_amount ELSE tran_amount END) AS Balance
            FROM part_tran pt join dtd on pt.transaction_header_id = dtd.sn
            join accounts a on a.acid=pt.acid
            where dtd.deleted_flag = 'N' AND dtd.posted_flag = 'Y'
            AND pt.tran_date <= DATE_ADD($P{to_date}, INTERVAL 1 DAY)
            AND a.gl_subhead IN (500, 110)
             AND (
            dtd.reversal_posted_flag IS NULL
            OR dtd.reversal_posted_flag = 'N'
            OR (dtd.reversal_posted_flag <> 'Y' AND dtd.reversed_flag <> 'Y')
        )
            GROUP BY a.acid) d ON a.acid = d.acid

    GROUP BY a.gl_subhead)
UNION ALL
    (SELECT 
        classification, a.acid, account_name, a.gl_subhead, gl_subhead_description, round(SUM(Balance), 2) AS bal,
        CASE
            WHEN classification = 'ASSETS' THEN 'debit'
            WHEN classification = 'INCOMES' THEN 'debit'
            WHEN classification = 'LIABILITIES' THEN 'credit'
            WHEN classification = 'EXPENSES' THEN 'credit'
            WHEN classification = 'SHARES' THEN 'credit'
            WHEN classification = 'RESERVE' THEN 'debit'
            ELSE NULL
        END AS correct_column 
    FROM accounts a
    JOIN glsubhead b ON a.gl_subhead = b.gl_subhead_code
    JOIN gl c ON c.gl_code = b.gl_code
    JOIN (SELECT a.acid, SUM(CASE WHEN part_tran_type = 'Debit' THEN -1 * tran_amount ELSE tran_amount END) AS Balance
            FROM part_tran pt join dtd on pt.transaction_header_id = dtd.sn
            join accounts a on a.acid=pt.acid
            where dtd.deleted_flag = 'N' AND dtd.posted_flag = 'Y'
             AND (
            dtd.reversal_posted_flag IS NULL
            OR dtd.reversal_posted_flag = 'N'
            OR (dtd.reversal_posted_flag <> 'Y' AND dtd.reversed_flag <> 'Y')
        )
      
 AND pt.tran_date <= DATE_ADD($P{to_date}, INTERVAL 1 DAY)
AND a.gl_subhead IN ('250','220') and a.account_type = 'OAB'
GROUP BY a.acid) d ON a.acid = d.acid

GROUP BY a.acid,a.gl_subhead)

union
 (SELECT 
classification, '220' as `a.acid`, 'Deposit Contributions' as account_name, a.gl_subhead, gl_subhead_description, round(SUM(Balance), 2) AS bal,
        CASE
            WHEN classification = 'ASSETS' THEN 'debit'
            WHEN classification = 'INCOMES' THEN 'debit'
            WHEN classification = 'LIABILITIES' THEN 'credit'
            WHEN classification = 'EXPENSES' THEN 'credit'
            WHEN classification = 'SHARES' THEN 'credit'
            WHEN classification = 'RESERVE' THEN 'debit'
            ELSE NULL
        END AS correct_column 
    FROM accounts a
    JOIN glsubhead b ON a.gl_subhead = b.gl_subhead_code
    JOIN gl c ON c.gl_code = b.gl_code
    JOIN (SELECT a.acid, SUM(CASE WHEN part_tran_type = 'Debit' THEN -1 * tran_amount ELSE tran_amount END) AS Balance
            FROM part_tran pt join dtd on pt.transaction_header_id = dtd.sn
            join accounts a on a.acid=pt.acid
            where dtd.deleted_flag = 'N' AND dtd.posted_flag = 'Y'
        AND (
            dtd.reversal_posted_flag IS NULL
            OR dtd.reversal_posted_flag = 'N'
            OR (dtd.reversal_posted_flag <> 'Y' AND dtd.reversed_flag <> 'Y')
        )
AND pt.tran_date <= DATE_ADD($P{to_date}, INTERVAL 1 DAY)
AND a.gl_subhead = '220'  AND a.account_type != 'OAB') d ON a.acid = d.acid

GROUP BY a.gl_subhead)




union

(SELECT 
classification, '250' as `a.acid`, 'Deposits' as account_name, a.gl_subhead, gl_subhead_description, round(SUM(Balance), 2) AS bal,
 CASE
        WHEN classification = 'ASSETS' THEN 'debit'
        WHEN classification = 'INCOMES' THEN 'debit'
        WHEN classification = 'LIABILITIES' THEN 'credit'
        WHEN classification = 'EXPENSES' THEN 'credit'
        WHEN classification = 'SHARES' THEN 'credit'
        WHEN classification = 'RESERVE' THEN 'debit'
        ELSE NULL  -- or provide a default value if needed
    END AS correct_column 
FROM accounts a
JOIN glsubhead b ON a.gl_subhead = b.gl_subhead_code
JOIN gl c ON c.gl_code = b.gl_code
JOIN (SELECT a.acid, SUM(CASE WHEN part_tran_type = 'Debit' THEN -1 * tran_amount ELSE tran_amount END) AS Balance, dtd.deleted_flag 
FROM part_tran pt join dtd on pt.transaction_header_id = dtd.sn
join accounts a on a.acid=pt.acid
where dtd.deleted_flag = 'N' AND dtd.posted_flag = 'Y'
 AND (
            dtd.reversal_posted_flag IS NULL
            OR dtd.reversal_posted_flag = 'N'
            OR (dtd.reversal_posted_flag <> 'Y' AND dtd.reversed_flag <> 'Y')
        )
 AND pt.tran_date <= DATE_ADD($P{to_date}, INTERVAL 1 DAY)
AND a.gl_subhead = 250 and a.account_type != 'OAB'
GROUP BY a.acid) d ON a.acid = d.acid

GROUP BY a.gl_subhead)) as subquery 
ORDER BY FIELD(classification, 'ASSETS', 'INCOMES', 'LIABILITIES', 'EXPENSES', 'SHARES', 'RESERVE'),  subquery.gl_subhead ASC, subquery.acid ASC;]]>
	</queryString>
