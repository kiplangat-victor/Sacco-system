	<queryString language="SQL">
		<![CDATA[select w.welfare_name, w.welfare_code, wa.action_name, pt.tran_date, pt.welfare_member_code, CAST((case when pt.part_tran_type='Debit' then tran_amount else 0.00 end) AS DECIMAL(65, 2)) as d_amount,
CAST((case when pt.part_tran_type='Credit' then tran_amount else 0.00 end) AS DECIMAL(65, 2)) as c_amount
 from part_tran pt join dtd on dtd.sn = pt.transaction_header_id join  welfare w on w.welfare_code = pt.welfare_code join welfare_action wa on pt.welfare_action = wa.action_code
  where pt.welfare_code = $P{welfare_code} and  pt.welfare_member_code = $P{customer_code} and dtd.posted_flag = 'Y' order by tran_date asc;]]>
