	<queryString>
		<![CDATA[select coalesce(ac.acid, '') as acid, coalesce(ld.adjustment_date) as adjustment_date, coalesce(ac.account_name, '') as account_name,
coalesce(ac.account_manager, '') as account_manager, coalesce(ld.adjustment_amount, 0.0) as adjustment_amount, coalesce(ld.demand_amount, 0.0) as demand_amount,
coalesce(br.branch_description, '') as branch_description, coalesce(ld.demand_type, '') as demand_type
from accounts ac join branch br on ac.sol_code=br.branch_code
join loan on loan.account_id_fk = ac.id and ac.acid =  $P{acid}
join loan_demand ld on loan.sn=ld.loan_sn and ld.adjustment_date between $P{from_date} and $P{to_date} order by ld.adjustment_date]]>
