	<queryString language="SQL">
		<![CDATA[SELECT ac.acid, ac.customer_code, ac.account_name, m.dob, ln.next_repayment_date, m.national_id, m.employer_code, m.employer_name, m.phone_number, ac.product_code, i.calculation_method, ln.number_of_installments, CONCAT(i.interestrate,' ', i.interest_period) AS interestrate, CONCAT(ln.frequency_period, IF(i.interest_period = 'p.m', ' MONTHS', 'YEARS')) AS frequency_period, ln.disbursement_date,ln.principal_amount, ln.maturity_date, ac.account_balance, COALESCE(arr.arrear_days, '0') AS arrear_days, demands.total_interest, ln.asset_classification from (SELECT * FROM loan WHERE  ($P{classification} = 'ALL' OR (asset_classification = $P{classification}))) ln join (SELECT * FROM accounts WHERE account_type = 'laa'  and ($P{product_code} = 'ALL' OR  product_code = $P{product_code} )  and (account_balance < -0.9 or account_balance > 0.9 ) and  opening_date between $P{from_date} and DATE_ADD($P{to_date}, INTERVAL 1 DAY)) ac on ac.id = ln.account_id_fk 
		left join  product p on p.product_code = ac.product_code 
		left join   interestcodeparams i on i.interest_code =  p.interest_table_code 
left join (select datediff(curdate(),min(demand_date)) arrear_days, loan_sn from loan_demand where adjustment_amount+1 < demand_amount and deleted_flag = 'N' group by loan_sn) as arr on ln.sn = arr.loan_sn 
left join (select datediff(curdate(),min(demand_date)) arrear_days, sum(interest_amount) as total_interest, loan_sn from loan_demand where  deleted_flag = 'N' group by loan_sn) as demands on ln.sn = demands.loan_sn 
left join  (SELECT * FROM (select unique_id  AS national_id, dob, coalesce(member.phone_number, '') as phone_number, CONCAT(coalesce(member.first_name, ''), ' ',coalesce(member.middle_name, ''), ' ' , coalesce(member.last_name, '')) as member_name, coalesce(member.customer_code, '') as customer_code, member.posted_time, coalesce(br.branch_description, '') as branch_description, coalesce(member.posted_by, '') as posted_by, coalesce(member.employer_code, '') as employer_code,  coalesce(member.employer_name, '') as employer_name  from retailcustomer member  join 
 branch br on br.branch_code=member.branch_code and member.deleted_flag='N' and br.deleted_flag='N' ) AS k
UNION
SELECT * FROM(select verification_doc_number AS national_id,  registration_date as dob,  coalesce(member.primary_phone, '') as phone_number, coalesce(member.group_name, '') as member_name, coalesce(member.customer_code, '') as customer_code, member.posted_time, coalesce(br.branch_description, '') as branch_description, coalesce(member.posted_by, '') as posted_by,  ' ' AS employer_code, ' ' AS employer_name from group_member member  join
branch br on br.branch_code=member.branch_code and member.deleted_flag='N' and br.deleted_flag='N' ) AS m) AS m ON m.customer_code = ac.customer_code  order by ac.customer_code
;]]>
