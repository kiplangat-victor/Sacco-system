	<queryString>
		<![CDATA[SELECT *, rel_amount AS relative_amount FROM(
SELECT distinct gl.classification AS cls, revs.total_reversal, loan.principal_amount, ac.opening_date, ac.customer_code, prd.product_code, prd.product_code_desc, ac.account_name, ac.acid,  COALESCE( CONCAT("(Installment Amt: ",loan.installment_amount, " )"), "") AS extra, ptdtd.dtd_sn, ptdtd.sn, ptdtd.transaction_code, ptdtd.tran_date, ptdtd.tran_particulars, ptdtd.c_amount, ptdtd.d_amount, ptdtd.rel_amount, ptdtd.tran_amount FROM (
select distinct pt.acid, pt.sn, dtd.sn as dtd_sn, dtd.transaction_code, 
pt.tran_date, tran_particulars,
CAST((case when pt.part_tran_type='Credit' then tran_amount else 0.00 end) AS DECIMAL(65, 2)) as c_amount,
CAST((case when pt.part_tran_type='Debit' then tran_amount else 0.00 end) AS DECIMAL(65, 2)) as d_amount,
CAST((case when pt.part_tran_type='Debit' then tran_amount*-1 else tran_amount end) AS DECIMAL(65, 2)) as rel_amount,
pt.tran_amount 
from part_tran pt join (SELECT * FROM dtd WHERE  transaction_code IN (select l2.transaction_code from loan_demand l1 
join loan_demand l2 where l1.demand_date < l2.demand_date and l1.loan_sn = l2.loan_sn and l1.deleted_flag = 'N' and  l2.deleted_flag = 'N' and l2.interest_amount > 0 and l1.demand_amount > l1.adjustment_amount+10 
and   l2.demand_amount > l2.adjustment_amount+10) AND sn IN(SELECT transaction_header_id FROM part_tran
 WHERE acid IN (SELECT acid FROM accounts WHERE account_type = 'laa'))) dtd on pt.transaction_header_id = dtd.sn and dtd.posted_flag = 'Y' 
 and (dtd.reversal_posted_flag  IS NULL OR dtd.reversal_posted_flag = 'N' OR (dtd.reversal_posted_flag <> 'Y' and dtd.reversed_flag <> 'Y'))  WHERE acid IN (SELECT acid FROM accounts WHERE  account_type = 'laa') 

) AS ptdtd
JOIN (
select 
SUM(CAST((case when pt.part_tran_type='Debit' then tran_amount*-1 else tran_amount end) AS DECIMAL(65, 2))) as total_reversal, acid 
from part_tran pt join (SELECT * FROM dtd WHERE  transaction_code IN (select l2.transaction_code from loan_demand l1 
join loan_demand l2 where l1.demand_date < l2.demand_date and l1.loan_sn = l2.loan_sn and l1.deleted_flag = 'N' and  l2.deleted_flag = 'N' and l2.interest_amount > 0 and l1.demand_amount > l1.adjustment_amount+10 
and   l2.demand_amount > l2.adjustment_amount+10) AND sn IN(SELECT transaction_header_id FROM part_tran
 WHERE acid IN (SELECT acid FROM accounts WHERE account_type = 'laa'))) dtd on pt.transaction_header_id = dtd.sn and dtd.posted_flag = 'Y' 
 and (dtd.reversal_posted_flag  IS NULL OR dtd.reversal_posted_flag = 'N' OR (dtd.reversal_posted_flag <> 'Y' and dtd.reversed_flag <> 'Y'))  WHERE acid IN (SELECT acid FROM accounts WHERE  account_type = 'laa') 
group by acid 
) AS revs ON ptdtd.acid =  revs.acid  
JOIN (SELECT * FROM accounts WHERE account_type = 'laa') ac  ON ac.acid = ptdtd.acid   
left join loan on loan.account_id_fk = ac.id 
left join (SELECT product_code, product_code_desc FROM product) AS prd ON ac.product_code = prd.product_code
join glsubhead ON glsubhead.gl_subhead_code = ac.gl_subhead
join gl ON gl.gl_code = glsubhead.gl_code 
) AS k where rel_amount <> 0 
order by opening_date asc, acid, tran_date asc, dtd_sn ASC, tran_amount DESC]]>
	</queryString>
